<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
  <link rel="stylesheet" href="css/style.css">


  <div class="container">
    <div class="row">
      <div class="col-lg-1"></div>
      <div class="col-lg-8 my-3">
        <h1 class="text-center">Seu Perfil</h1>
        <p class="text-center lead">Seus dados est√£o abaixo</p>

        <nav>
          <div class="container-fluid">
            <form class="d-flex" role="search" action="/pesquisa" method = "get">
              <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
              <button class="btn btn-outline-success" type="submit">Search</button>
            </form>
          </div>
        </nav>
        

        <div class="responsive-table">
          <table class="table my-3 text-light" color>
            <tr>
              <td>CPF</td>
              <td>Nome</td>
              <td>Email</td>
              <td>Password</td>
              <td>Telefone</td>
              <td>Message</td>
              <td>Country</td>
              <td><img src="img/lixeira2.png" alt=""></td>
              <td><img src="img/editar3.png" alt=""></td>
            </tr>

            <% users.forEach(user=> { %>

              <tr>
                <td>
                  <%= user.cpf%>
                </td>
                <td>
                  <%= user.nameuser%>
                </td>
                <td>
                  <%= user.email%>
                </td>
                <td>
                  <%= user.password%>
                </td>
                <td>
                  <%= user.tel%>
                </td>
                <td>
                  <%= user.msg%>
                </td>
                <td>
                  <%= user.country%>
                </td>
                <td> <a href=""> <img class="img_listar"  src="img/lixeira2.png" alt=""> </a> </td>
                <td> <a href=""> <img class="img_listar" src="img/editar3.png" alt=""> </a> </td>
              </tr>

              <% }) %>

          </table>
          <hr>
          <a href="/" class="btn btn-dark">Voltar</a>
        </div>
      </div>
      <div class="col-lg-2"></div>
    </div>
  </div>